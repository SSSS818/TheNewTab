{
  "manifest_version": 2,
  "name": "TheNewTab",
  "version": "1.0.0",
  "description": "A brilliant newtab",
  "icons": {
    "96": "src/images/icon-96.png"
  },
  "browser_action": {
    "default_icon": "src/images/icon-96.png",
    "default_popup": "./src/popup.html",
    "default_title": "TheNewTab",
    "browser_style": true
  },
  "applications": {
    "gecko": {
      "id": "tracyliubai@gmail.com",
      "strict_min_version": "55.0a1",
      "strict_max_version": "58.*"
    }
  },
  "permissions": [
    "<all_urls>",
    "downloads",
    "storage",
    "tabs",
    "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "./src/index.html"
  }
}